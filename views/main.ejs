                  <span class="glyphicon glyphicon-map-marker"></span><%= ip %><!-- 140.112.xx -->
                </div>
              </div>
            </div>  <!-- col-md-3 -->
            <div class="col-md-3 main s20">
              <div>
                <span class="glyphicon glyphicon-tag yellow"></span>發
                <span class="label label-danger left">
                  <%= totalpost %>
                </span>
              </div>
              <div>
                <span class="glyphicon glyphicon-tag yellow"></span>推
                <span class="label label-danger left">
