                <span class="glyphicon glyphicon-stats"></span>
              </div> <!-- page-header -->
            </div>
          </div>

      </div> <!-- row -->


<!-- news part -->
<!-- ================================================= -->
      <div class="row top">
        <div class="col-md-8 ">
          <div class="col-md-8 col-md-offset-1 ">
            <div class="table-responsive top">
                <table class="table ">
                    <tbody>

                      <% for (var i=0;i<(list.length);i++) {%>

                        <tr>
                          <td> <span class="glyphicon glyphicon-hand-right"></span></td>
                            <td class="fontt">
                              <%= list[i].bbs %> 
                            </td>
                              <% if (list[i].ip!=undefined) { %>
                                
                                <td class="fontt yellow">發
                                </td>

                              <% } else {%>    
                                <% if (list[i].res[0]>list[i].res[1]) { %>
                                  
                                  <td class="fontt yellow">推
                                  </td>

                                <%  } else if (list[i].res[0]===list[i].res[1]) { %>
                                  
                                  <td class="fontt red">→
                                  </td>

                                <% } else { %>
                                 
                                  <td class="fontt red">噓
                                  </td>

                                 <% } %>
                              <% } %>

                            <td class="">
                                <div class="fonttt text-left">
                                  <a class="various fancybox.iframe" href='
                                  <%= list[i].url %>
                                  '>
                                  <%= list[i].title %>
                                  </a></div>
                                <div class="grey text-left">
                                  <cite>
                                    <%= list[i].time %>
                                  </cite>
                                </div>
                            </td>
                        </tr>

                      <% } %>
                    </tbody>
                    
                </table>
              </div>  <!-- table -->
            </div> <!-- col-md-12 table data container -->
          </div> <!-- col-md-8 table container-->
