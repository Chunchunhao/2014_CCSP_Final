            <div class="tab-content">
              <div class="tab-pane active" id="all">
                <div class="table-responsive top">
                  <table class="table ">
                    <tbody>
                      <% for (var i=0;i<(list.length);i++) {%>
                        <tr>
                          <td> <span class="glyphicon glyphicon-hand-right"></span></td>
                            <td class="fontt">
                              <%= list[i].bbs %> 
                            </td>
                              <% if (list[i].ip!=undefined) { %>
                                
                                <td class="fontt green">發
                                </td>

                              <% } else {%>    
                                <% if (list[i].res[0]>list[i].res[1]) { %>
                                  
                                  <td class="fontt yellow">推
                                  </td>

                                <%  } else if (list[i].res[0]===list[i].res[1]) { %>
                                  
                                  <td class="fontt red">→
                                  </td>

                                <% } else { %>
                                 
                                  <td class="fontt red">噓
                                  </td>

                                 <% } %>
                              <% } %>

                            <td class="">
                                <div class="fonttt text-left">
                                  <a class="various fancybox.iframe" href='<%= list[i].url%>' title='正在搜尋的id：<%= id %>'>
                                  <%= list[i].title %>
                                  </a></div>
                                <div class="grey text-left">
                                  <cite>
                                    <%= list[i].time %>
                                  </cite>
                                </div>
                            </td>
                        </tr>

                      <% } %>
                    </tbody>
                    
                </table>
